{
    "name": "inboxroad/inboxroad-php",
    "version": "1.0.1",
    "description": "Inboxroad Library for PHP",
    "homepage": "https://www.inboxroad.com",
    "autoload": {
        "psr-4": {
            "Inboxroad\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Inboxroad\\Test\\": "test/"
        }
    },
    "require": {
        "php": ">=8.2",
        "ext-json": "*",
        "psr/http-client": "^1.0",
        "guzzlehttp/guzzle": "^7.10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5.43",
        "friendsofphp/php-cs-fixer": "^3.89.1",
        "phpstan/phpstan": "^2.1.31",
        "rector/rector": "^2.2"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit",
        "php-cs-fixer": "./vendor/bin/php-cs-fixer fix .",
        "phpstan": "./vendor/bin/phpstan analyse src test -c phpstan.neon --level max --memory-limit=-1",
        "rector": "./vendor/bin/rector src test"
    },
    "license": "MIT"
}
